<h1> articles form </h1>
<%= form_for :article, url: articles_path, html: { multipart: true } do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
	<%= f.label :fimage %><br>
	<%= f.file_field :fimage %>
  </p> 
  <p>
    <%= f.submit %>
  </p>
<% end %>
